<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <welcome-file-list>
    <welcome-file>Welcome.jsp</welcome-file>
    </welcome-file-list>
    
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>add_mgmnt.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
     <servlet>
        <servlet-name>LoginAuth</servlet-name>
        <servlet-class>add_mgmnt.LoginAuth</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginAuth</servlet-name>
        <url-pattern>/login_auth</url-pattern>
    </servlet-mapping>
   <servlet>
        <servlet-name>AddressList</servlet-name>
        <servlet-class>add_mgmnt.AddressList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddressList</servlet-name>
        <url-pattern>/addr_list</url-pattern>
    </servlet-mapping>
</web-app>
